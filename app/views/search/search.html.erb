<div class="container">
  <section class="page search">
    <div class="page__header page__header--center">
      <h1 class="page__title">Veja o que encontramos</h1>
    </div>

    <%# <div id="carouselExampleControls" class="carousel slide text-center carousel-dark" data-mdb-ride="carousel"> %>
      <div class="show-contact">
          <% if @result_search.present? %>
            <% @result_search.each do |result_search| %>
            <div class="show-contact-card">
              <div class="show-contact-image">
                <% if result_search.icon.present? %>
                  <%= image_tag(result_search.icon.url, class:"rounded-circle shadow-1-strong mb-4") %>
                <%end%>
              </div>
                  <p><%= result_search.name%></p>
                  <p>  <%= result_search.phone%> </p>
                  <p>  <%= result_search.email%> </p>
                  <p>  <%= result_search.company%></p>
              </div>
            <% end %>
          <%end%>
        </div>
      </div>
   

    <div class="btn">
      <%# Volta a todos os formulÃ¡rios %>
      <button type="button" class="btn btn-primary"> <%= link_to "Voltar", formularios_path %></button>
    </div>
  </section>
</div>

 
